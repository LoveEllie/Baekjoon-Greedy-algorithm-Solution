    "n, m, k = map(int, input().split())\n",
    "\n",
    "team = 0\n",
    "\n",
    "while n>=2 and m>=1 and n+m>=k+3:\n",
    "    n-=2\n",
    "    m-=1\n",
    "    team+=1\n",
    "\n",
    "print(team)"
